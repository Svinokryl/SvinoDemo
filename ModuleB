**RTR-COD**
en
	conf
		hostname rtr-cod.cod.ssa2026.region;exec bash
		int int 0
			178.207.179.4/28
			ip nat out
			int int1
			ip add 192.168.32.201/30
			ip nat ins
			port te0
			service-instance int0/te0
				enc unt
				conn ip int int0
				port te1
			service-instance int1/te1
				enc unt 
				conn ip int int1
				end
	wr
	conf
		router bgp 64500
			bgp router-id 178.207.179.4
			neig 178.207.179.1 remote-as 31133
			ex
		int tunnel.0
			ip add 10.10.10.1/30
			ip mtu 1476
			ip tun 178.207.179.4 178.179.207.28 mode gre
			ex
		ip nat pool cod 192.168.32.1-192.168.32.254
		ip nat source dy so cod overload int int0
		end
	wr
	conf
		router ospf 1
			router id 10.10.10.1
			passive-int default
			no passive-int tunnel.0
			net 10.10.10.0/30 area 0
			net 192.168.32.200/30 area 0
			ex
		int tunnel.0
			ip ospf authentication message-diggest
			ip ospf message-digest-key 1 md5 P@ssw0rd
			end
	wr
	conf

**RTR-A**
en
	conf
		hostname rtrt-a
		ip domain-name office.ssa2026.region
		int int0
			ip nat out 
			ip add 178.207.179.28/28
			int int1
			ip nat ins
			ip add 192.168.44.1/29
			int int2
			ip nat ins
			ip add 192.168.42.9/29
			ip nat ins
			ip add 192.168.43.17/29
			port te0
			service-ins int0/te0
				enc unt
				conn ip int int0
				port te1
			service-ins int1/te1
				enc dot 100 exa
				rew pop 1
				conn ip int int1
				ex
			service-ins int2/te1
				enc dot 200 exa
				rew pop 1
				conn ip int int2
				ex
			service-ins 3
				enc dot 300 exa
				rew pop 1
				conn ip int int3
				end
	conf
		ip nat pool office 192.168.44.1-192.168.44.254
		ip nat souce dynamic inside--- pool vl100 overload int int0
		ip route 0.0.0.0/0 178.207.179.25
		int tunnel.0
			ip add 10.10.10.2/30
			ip mtu 1476
			ip tun 178.207.179.28 178.207.179.4 mode gre
			ex
		router ospf 1
			ospf router-id 10.10.10.2
			passive-int default
			net 10.10.10.0/30 area 0
			net 192.168.44.0 0.0.0.3 area 0
			net 192.168.42.8 0.0.0.3 area 0
			net 192.168.43.16 0.0.0.3 area 0
			no passive-int tunnel.0
			ex
		int tunnel.0
			ip ospf auth message-digest
			ospf message-digest-key 1 md5 P@ssw0rd
			end
	wr

**SW1-A**
systemctl enable --now openvswitch
hostnamectl set-hostname sw1-a.office.ssa2026.region;exec bash
cd /etc/net/ifaces
mkdir enp7s{2,3,4}
vim enp7s1/options
	`TYPE=eth`
	`BOOTPROTO=static`
cp enp7s1/options enp7s2/options
cp enp7s1/options enp7s4/options
cp enp7s1/options enp7s4/options
vim default/options
	`OVS_REMOVE=no`
systemctl restart network
mkdir mgmt
cd mgmt
vim options
	`TYPE=ovsport`
	`BOOTPROTO=static`
	`CONFIG_IPV4=yes`
	`VID=300`
	`BRIDGE=sw1-a`
ovs-vsctl add-br sw1-a
echo 192.168.44.17/24 > ipv4address
echo default via 192.168.44.16 > ipv4route
systemctl restart network
ovs-vsctl add-port sw1-a enp7s1 trunk=100,200,300
ovs-vsctl add-port sw1-a enp7s2 trunk=100,200,300
ovs-vsctl add-port sw1-a enp7s3 trunk=100,200,300
ovs-vsctl add-port sw1-a enp7s4 tag=100
modprobe 8021q
ovs-vsctl set-bridge sw1-a rstp_enable=true
ovs-vsctl set bridge sw1-a other_config:stp-protocol=rstp
ovs-vsctl set bridge sw1-a other_config:stp-priority=0
ovs-appctl stp/show
ovs-vsctl set port mgmt vlan_mode=native-untagged

