hostnamectl set-hostname sw1-cod.cod.ssa2026.region;exec bash
cd /etc/net/ifaces
mkdir enp7s{2,3,4,5,6}
vim enp7s1/options
	`TYPE=eth`
	`BOOTPROTO=static`
cp enp7s1/options enp7s2/options
cp enp7s1/options enp7s4/options
cp enp7s1/options enp7s4/options
cp enp7s1/options enp7s5/options
cp enp7s1/options enp7s6/options
vim default/options
	`OVS_REMOVE=no`
systemctl restart network
mkdir mgmt
cd mgmt
vim options
	`TYPE=ovsport`
	`BOOTPROTO=static`
	`CONFIG_IPV4=yes`
	`VID=300`
	`BRIDGE=sw1-cod`
echo 192.168.32.18/24 > ipv4address
echo default via 192.168.32.17 > ipv4route
ovs-vsctl add-br sw1-cod
systemctl restart network
ovs-vsctl add-port sw1-cod enp7s1 trunk=100,200,300,400,500
ovs-vsctl add-bond sw1-cod bond0 enp7s2 enp7s3 bond_mode=active-backup
ovs-vsctl set port bond0 trunk=100,200,300,400,500
ovs-vsctl add-port sw1-cod enp7s4 tag=100
ovs-vsctl add-port sw1-cod enp7s5 tag=200
ovs-vsctl add-port sw1-cod enp7s6 tag=400
modprobe 8021q
ovs-vsctl set-port mgmt vlan_mode=native-untagged

