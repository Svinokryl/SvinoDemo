**SRV1-COD**
apt-get update && apt-get install bind bind-utils -y
cd /etc/bind
vim options.conf
	listen-on { any; };
	forwarders { 100.100.100.100; };
	allow-querry { 0.0.0.0/0; };
	allow-transfer { 192.168.41.2; };
vim rfc1912.local
	`zone "cod.ssa2026.region" {`
		`type master;`
		`file "cod.ssa2026.region";`
		`allow-transfer { 192.168.41.2; };`
	`};`
	`zone "3.in-addr.arpa." {`
		`type master;`
		`file "cod.ssa2026.region";`
		`allow-transfer { 192.168.41.2; };`
	`};`
	`zone "office.ssa2026.region" {`
		`type forward;`
		`forward only;`
		`forwarders { 192.168.41.2; };`
	`};`
	`zone "4.in-addr.arpa" {`
		`type forward;`
		`forward only;`
		`forwarders { 192.168.41.2; };`
	`};`
cp localhost cod.ssa2026.region
chown root:named cod.ssa2026.region
	![[Pasted image 20260201113048.png]]
	![[Pasted image 20260201113059.png]]
vim  /etc/net/ifaces/enp7s1/resolv.conf
	search cod.ssa2026.region
	nameserver 127.0.0.1
systemctl restart network
systemctl restart bind

